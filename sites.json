{
  "chatgpt": {
    "availFeatures": [ "fullerWindows", "fullWindow", "hiddenFooter", "hiddenHeader",
      "notifDisabled", "ncbDisabled", "tcbDisabled", "widerChatbox", "wideScreen" ],
    "selectors": {
      "btns": {
        "newChat": "button[data-testid*=\"new-chat-button\"]",
        "send": "button[data-testid=\"send-button\"]",
        "sidebarToggle": "button[data-testid*=\"sidebar-button\"]"
      },
      "header": "main .sticky", "input": "#prompt-textarea", "sidebar": "[class*=\"sidebar\"]"
    }
  },
  "perplexity": {
    "availFeatures": [ "fullerWindows", "fullWindow", "hiddenHeader",
      "notifDisabled", "ncbDisabled", "tcbDisabled", "wideScreen" ],
    "hasSidebar": true,
    "selectors": {
      "btns": {
        "login": "button[data-testid*=\"sign-in\"]", "newChat": "div.sticky [class*=\"ring\"]",
        "send": "button:has([d^=\"M209.4\"], [d^=\"M80 112a32\"], [d^=\"M440.6\"])",
        "sidebarToggle": "button:has([d^=\"M48 88c0-13\"], [d^=\"M0 424c0\"])"
      },
      "header": "div.sticky:nth-of-type(2)", "input": "textarea[placeholder]", "sidebar": "div.sticky"
    }
  },
  "poe": {
    "availFeatures": [ "fullerWindows", "fullWindow", "hiddenHeader",
      "notifDisabled", "ncbDisabled", "tcbDisabled", "widerChatbox", "wideScreen" ],
    "hasSidebar": true,
    "selectors": {
      "btns": { "newChat": "header a[class*=\"button\"]" },
      "header": "header",
      "input": "[class*=\"InputContainer_textArea\"] textarea, [class*=\"InputContainer_textArea\"]::after",
      "sidebar": "menu[class*=\"sidebar\"], aside[class*=\"sidebar\"]"
    }
  }
}
